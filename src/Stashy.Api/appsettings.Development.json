{
  "Serilog": {
    "Using": ["Serilog.Sinks.Console"],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      { "Name": "Console" }
    ],
    "Cors": {
      "Origins": [
        "http://localhost:8080"
      ]
    },
    "Enrich": ["FromLogContext", "WithMachineName", "WithThreadId"]
  }
}